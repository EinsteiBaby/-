page {
  background-color: #fff;
  position: fixed;
}

.logo {
  position: absolute;
  top: 4rem;
  left: calc(50% - 2.5rem);
}

.logo image {
  width: 5rem;
  height: 6rem;
}

@font-face {
  font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_966168_geij5wkiu9o.eot?t=1544767205191'); /* IE9*/
  src: url('//at.alicdn.com/t/font_966168_geij5wkiu9o.eot?t=1544767205191#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
         url('//at.alicdn.com/t/font_966168_geij5wkiu9o.ttf?t=1544767205191') format('truetype'),
