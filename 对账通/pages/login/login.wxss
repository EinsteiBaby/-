  -moz-osx-font-smoothing: grayscale;
}

.icon-zhanghu:before {
  content: "\e600";
  padding-right: 40rpx;
  font-size: 1.1rem;
}

.icon-mima:before {
  content: "\e601";
  padding-right: 40rpx;
  font-size: 1.1rem;
}

.login {
  position: absolute;
  padding-left: 10%;
