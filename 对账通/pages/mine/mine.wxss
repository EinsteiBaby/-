page {
  position: fixed;
}

.photo image {
  width: 4rem;
  height: 4rem;
  border-radius: 5rem;
  border: 0.1rem solid #fff;
}

.info {
  background-color: #ffc900;
  position: absolute;
  width: 100%;
  height: 8rem;
}

.photo {
  display: inline-block;
  vertical-align: middle;
  position: absolute;
  left: 1rem;
}

.detail {
  display: inline-block;
  vertical-align: middle;
  position: absolute;
  top: 1rem;
  left: 6.5rem;
  font-size: 0.8rem;
}

.signature {
  color: #666;
  font-size: 0.7rem;
}

.credit {
  background-color: #fff;
  position: absolute;
  top: 5.5rem;
  left: calc(50% - 350rpx);
  border-radius: 0.2rem;
  height: 5rem;
  width: 700rpx;
  box-shadow: 1px 1px 5px #f90;
  text-align: center;
}

.inner {
  margin-top: 1rem;
}

.func {
  position: absolute;
  width: 100%;
  top: 11.5rem;
}

.exit {
  position: absolute;
  width: 100%;
  top: 25rem;
}

@font-face {
  font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_966168_zbn655ddkob.eot?t=1545025509700'); /* IE9*/
  src: url('//at.alicdn.com/t/font_966168_zbn655ddkob.eot?t=1545025509700#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
         url('//at.alicdn.com/t/font_966168_zbn655ddkob.ttf?t=1545025509700') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+*/ url('//at.alicdn.com/t/font_966168_zbn655ddkob.svg?t=1545025509700#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 20px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-lianxikefu:before {
  content: "\e668";
  font-weight: bold;
}

.icon-bianji:before {
  content: "\e61e";
}

.icon-zhuxiao:before {
  content: "\e66c";
  font-size: 22px;
  font-weight: bold;
}

.icon-bangzhudeng-xianxing:before {
  content: "\e797";
  font-weight: bold;
}

.icon-fankui-:before {
  content: "\e645";
  font-weight: bold;
}

.txt {
  font-size: 0.8rem;
}

.count {
  font-weight: bold;
  font-size: 1.2rem;
}

.billCount {
  border-right: 1px solid #eee;
}

.useDay {
  border-right: 1px solid #eee;
}

.outter {
  background-color: rgba(0, 0, 0, 0.8);
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  text-align: center;
  font-size: 0.9rem;
  color: rgba(0, 0, 0, 0.6);
}

.inner_ {
  position: absolute;
  width: 700rpx;
  height: 500rpx;
  background-color: #fff;
  border-radius: 0.5rem;
  left: calc(50% - 350rpx);
  top: 250rpx;
}

.inner_ image {
  margin-top: 50rpx;
  width: 100rpx;
  height: 100rpx;
}

.inner_ button {
  margin-top: 80rpx;
  width: 550rpx !important;
  border-radius: 0.1 !important;
}

.wel {
  margin-top: 1.5rem;
}

.doubt {
  margin-top: 0.3rem;
  font-size: 0.7rem;
  color: rgba(0, 0, 0, 0.6);
  text-decoration: underline;
}
