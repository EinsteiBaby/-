.demo-container {
  width: 85vw;
  height: 100vh;
  background: #fff;
  z-index: 999999;
}

.btn {
  position: fixed;
  bottom: 0;
  width: 100%;
}

.header {
  background-color: #ffc900;
  height: 100rpx;
  position: fixed;
  top: 0;
  width: 100vw;
}

.header input {
  background-color: #fff;
  border: 1px solid #eee;
  border-radius: 0.2rem;
  width: 72vw;
  margin-left: 0.5rem;
  margin-top: 0.1rem;
  display: inline-block;
  height: 2rem;
  font-size: 0.8rem;
  vertical-align: middle;
  padding-left: 2rem;
}

@font-face {
  font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_966168_c5jfqm0nneh.eot?t=1545106110049'); /* IE9*/
  src: url('//at.alicdn.com/t/font_966168_c5jfqm0nneh.eot?t=1545106110049#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
         url('//at.alicdn.com/t/font_966168_c5jfqm0nneh.ttf?t=1545106110049') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+*/ url('//at.alicdn.com/t/font_966168_c5jfqm0nneh.svg?t=1545106110049#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 58rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-shaixuan:before {
  margin-top: 0.5rem;
  vertical-align: middle;
  content: "\e644";
  display: inline-block;
  margin-left: 1rem;
  /* font-weight: bold; */
  color: #fff;
}

.icon-sousuo:before {
  font-size: 1.1rem;
  position: absolute;
  left: 0.9rem;
  top: 30rpx;
  color: rgba(0, 0, 0, 0.5);
  content: "\e682";
}

.content {
  position: absolute;
  width: 100vw;
  top: 120rpx;
  z-index: -1;
}

.info {
  background-color: #fff;
  margin-left: 0.8rem;
  margin-right: 0.8rem;
  border-radius: 0.3rem;
  margin-bottom: 0.8rem;
}

.num {
  padding-top: 0.5rem;
  padding-bottom: 0.5rem;
  margin-left: 0.5rem;
  margin-right: 0.5rem;
  border-bottom: 2px solid #eee;
  font-size: 1rem;
}

page {
  color: #333;
}

.infoContent {
  color: #999;
  font-size: 0.8rem;
  padding: 0.5rem;
}

.infoContent view {
  margin-bottom: 0.2rem;
}

.checkStatus {
  float: right;
  margin-right: 0.2rem;
  color: #cd2e1d;
  font-size: 1rem;
}

.hide {
  display: none;
}

.infoContent text {
  float: right;
  margin-right: 0.1rem;
}
